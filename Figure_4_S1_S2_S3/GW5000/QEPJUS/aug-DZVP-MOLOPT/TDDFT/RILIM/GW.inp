     !        TDA OFF
     !      &END BSE 
     !    &END GW	
     !  &END RI_RPA
     !&END WF_CORRELATION
    &END XC
    &PRINT
    &OVERLAP_CONDITION ON
    DIAGONALIZATION T
    &END OVERLAP_CONDITION
    &END PRINT
  &END DFT
