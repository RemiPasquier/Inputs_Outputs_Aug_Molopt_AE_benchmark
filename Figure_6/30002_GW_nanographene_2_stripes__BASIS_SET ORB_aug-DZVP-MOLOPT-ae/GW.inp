&GLOBAL
	PROJECT_NAME MOLNAME
	RUN_TYPE ENERGY
    EXTENDED_FFT_LENGTHS
	&TIMINGS
		THRESHOLD 0.005
	&END TIMINGS
&END GLOBAL

&FORCE_EVAL
	METHOD QS
	&DFT
		BASIS_SET_FILE_NAME BASIS_augMOLOPT
		BASIS_SET_FILE_NAME BASIS_cc_pVDZ
		POTENTIAL_FILE_NAME ALL_POTENTIALS
		&PRINT
		  &OVERLAP_CONDITION
		  	DIAGONALIZATION
		  &END OVERLAP_CONDITION
		  &MO
			ENERGIES
		  &END MO
		&END PRINT
		&MGRID
			CUTOFF 600
			REL_CUTOFF 60
		&END MGRID
		&QS
			EPS_DEFAULT 1.0E-15
			METHOD GAPW
		&END QS
		&SCF
			SCF_GUESS RESTART
			EPS_SCF 1e-6
			MAX_SCF 200
            CHOLESKY OFF
            ADDED_MOS 100
			&SMEAR ON
				METHOD FERMI_DIRAC
				ELECTRONIC_TEMPERATURE [K] 300 
			&END SMEAR    
			&MIXING
				METHOD BROYDEN_MIXING
				ALPHA 0.1
				NBROYDEN 4
			&END MIXING
		&END SCF
		&XC
			&XC_FUNCTIONAL PBE
			&END XC_FUNCTIONAL
		&END XC
		&POISSON
			PERIODIC NONE
			POISSON_SOLVER MULTIPOLE
		&END POISSON
	&END DFT
	&PROPERTIES
		&BANDSTRUCTURE
			&GW
                NUM_TIME_FREQ_POINTS         15
                MEMORY_PER_PROC              6
                CUTOFF_RADIUS_RI             7
				HEDIN_SHIFT T
			&END GW
		&END BANDSTRUCTURE
	&END PROPERTIES
	&SUBSYS
		&CELL
			PERIODIC NONE
			ABC 21.19 21.28 13.55
		&END CELL
		&TOPOLOGY
			&CENTER_COORDINATES
			&END CENTER_COORDINATES
			COORD_FILE_FORMAT XYZ
			COORD_FILE_NAME Nanographene_2.xyz
		&END TOPOLOGY
		&KIND H
			POTENTIAL ALL
			BASIS_SET ORB aug-DZVP-MOLOPT-ae
			BASIS_SET RI_AUX RI_aug-DZVP-MOLOPT-ae_N_RI_009_s_p_d_f_g_h_i_3_2_0_0_0_0_0_error_4.4e-05
		&END KIND
		&KIND C
			POTENTIAL ALL
			BASIS_SET ORB aug-DZVP-MOLOPT-ae
			BASIS_SET RI_AUX RI_aug-DZVP-MOLOPT-ae_N_RI_035_s_p_d_f_g_h_i_6_4_2_1_0_0_0_error_4.2e-04
		&END KIND
	&END SUBSYS
&END FORCE_EVAL
